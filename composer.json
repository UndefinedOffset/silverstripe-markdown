	"type": "silverstripe-module",
	"keywords": ["silverstripe", "markdown"],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Ed Chipman",
			"homepage": "http://www.edchipman.ca",
			"role": "Developer"
		}
	],

	"require":
	{
		"silverstripe/framework": "3.*",
		"composer/installers": "*"
	},
	"support": {
